{
  "version": 2,
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "headers": { "cache-control": "public,max-age=31536000,immutable" },
      "dest": "/_next/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "buildCommand": "pnpm install --no-frozen-lockfile && pnpm build",
  "installCommand": "pnpm install --no-frozen-lockfile"
}